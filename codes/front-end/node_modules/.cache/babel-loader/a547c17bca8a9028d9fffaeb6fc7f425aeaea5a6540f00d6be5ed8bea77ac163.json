e>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      userData: {\r\n        username: '',\r\n        password: '',\r\n        nickname: '',\r\n        gender: '',\r\n        telephone: ''\r\n      },\r\n      registrationResult: ''\r\n    };\r\n  },\r\n  methods: {\r\n    registerUser() {\r\n      axios.post('http://192.168.184.1:8000/tms/register', this.userData)\r\n          .then(response => {\r\n            this.registrationResult = response.data.message;\r\n          })\r\n          \r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAqCA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,QAAA;QACAC,QAAA;QACAC,QAAA;QACAC,MAAA;QACAC,SAAA;MACA;MACAC,kBAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAA;MACAV,KAAA,CAAAW,IAAA,gDAAAT,QAAA,EACAU,IAAA,CAAAC,QAAA;QACA,KAAAL,kBAAA,GAAAK,QAAA,CAAAZ,IAAA,CAAAa,OAAA;MACA;IAEA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}